{
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.AspNetCore": "Warning"
		}
	},
	"AllowedHosts": "*",
	"PriceAPIs": {
		"AVGCalculationFormula": "basic-avg",
		"Bitstamp": {
			"Url": "https://www.bitstamp.net/api/v2/ohlc/{0}/?step=3600&limit=1&start={1}",
			"TimestampType": "unix-timestamp",
			"TimestampFormat": "seconds"
		},
		"Bitfinex": {
			"Url": "https://api-pub.bitfinex.com/v2/candles/trade:1h:t{0}/hist?start={1}&limit=1&sort=+1",
			"TimestampType": "unix-timestamp",
			"TimestampFormat": "milliseconds"
		}
	}
}
